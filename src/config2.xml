<config>
	<services>
		<service id="1">
			<url>http://api.convenios.gov.br/siconv/v1/consulta/propostas.xml
			</url>
			<refresh-rate>1800</refresh-rate>
			<return-type>xml</return-type>
		</service>
	</services>

	<resources>
		<!-- -->
		<resource id="1" type="static">
			<uri>http://greco.ppgi.ufrj.br/resource#teste123</uri>
			<rdf-type>http://greco.ppgi.ufrj/ontology/concepts#novela </rdf-type>

			<static-literals>
				<static-literal>
					<value>globo</value>
					<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasChannel
					</parent-property>
				</static-literal>
			</static-literals>

			<dynamic-literals>
				<dynamic-literal service-id="1" type="unique">
					<group>//proposta</group>
					<literal>
						<value><![CDATA[/data_cadastramento_proposta/text()]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasUpdatedTimeStamp
						</parent-property>
					</literal>
				</dynamic-literal>
			</dynamic-literals>

			<object-properties>
				<object-property type="dominant">
					<property>http://greco.ppgi.ufrj/ontology/properties#Description
					</property>
					<object-uri>http://dbpedia.org/resource/Novela</object-uri>
				</object-property>
			</object-properties>

		</resource>

		<resource id="2" type="dynamic">

			<uri>http://greco.ppgi.ufrj.br/resource#</uri>
			<rdf-type>http://greco.ppgi.ufrj/ontology/concepts#twitter</rdf-type>


			<static-literals>
			</static-literals>

			<dynamic-literals>
				<dynamic-literal service-id="1">
					<group>//proposta</group>

					<literal>
						<value><![CDATA[/justificativa_resumida/text()]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasJustificativa
						</parent-property>
					</literal>

					<literal>
						<value><![CDATA[/valor_global/text()]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasValor
						</parent-property>
					</literal>
				</dynamic-literal>
			</dynamic-literals>

			<object-properties>
				<object-property type="nondominant">
					<property>http://greco.ppgi.ufrj/ontology/properties#hasMessage
					</property>
					<object-uri>http://greco.ppgi.ufrj.br/resource#teste12</object-uri>
				</object-property>
			</object-properties>
		</resource>
	</resources>
</config>