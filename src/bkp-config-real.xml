			<refresh-rate>0 30 * * * ? *</refresh-rate>
			<return-type>csv</return-type>
		</service>
		<service id="2">
			<url>http://www.camara.gov.br/sitcamaraws/deputados.asmx/ObterDeputados</url>
			<refresh-rate>0 57 * * * ? *</refresh-rate>
			<return-type>xml</return-type>
		</service>
