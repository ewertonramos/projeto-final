			<uri>http://greco.ppgi.ufrj.br/resource/AcidenteDeTrabalho#</uri>
			<rdf-type>http://greco.ppgi.ufrj.br/ontology/concepts#AcidentesTrabalho</rdf-type>
			
			<static-literals>
			</static-literals>
			
			<headers>
				<header>
					<index>1</index>
