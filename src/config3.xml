					</literal>
					
					<literal>
						<value><![CDATA[id]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasId</parent-property>
					</literal>
				</dynamic-literal>
			</dynamic-literals>

			<object-properties>
				<object-property type="dominant">
					<property>http://greco.ppgi.ufrj/ontology/properties#hasAlgumaProperty</property>
					<object-uri>http://greco.ppgi.ufrj.br/resource#object_uri</object-uri>
