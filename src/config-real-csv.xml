<config>
	<services>
		<service id="1">
			<url>http://api.dataprev.gov.br/previdencia/anuario/2009/acidentes-do-trabalho.csv</url>
			<refresh-rate>0 0/10 * * * ? *</refresh-rate>
			<return-type>csv</return-type>
		</service>
		<service id="2">
			<url>http://api.convenios.gov.br/siconv/v1/consulta/municipios.csv?</url>
			<refresh-rate>0 0/10 * * * ? *</refresh-rate>
			<return-type>csv</return-type>
		</service>
	</services>
	
	<resources>
		<resource id="1" type="static"  separator-char="," with-header="true">
			<uri>http://greco.ppgi.ufrj.br/resource#Estado</uri>
			<rdf-type>http://greco.ppgi.ufrj.br/ontology/concepts#Estado</rdf-type>
			
			
			<static-literals>
				<static-literal>
					<value>Unidades Federativas do Brasil</value>
					<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasDescricao</parent-property>
				</static-literal>
			</static-literals>
			<headers>
				<header>
					<index>8</index>
					<name>uf</name>
				</header>
				<header>
					<index>6</index>
					<name>uf_nome</name>
				</header>
				<header>
					<index>7</index>
					<name>uf_regiao</name>
				</header>
			</headers>

			<dynamic-literals>
				<dynamic-literal service-id="2" >
				<!-- type="unique" -->
					<group>uf</group>
					
					<literal>
						<value><![CDATA[uf]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasUF</parent-property>
					</literal>
					<literal>
						<value><![CDATA[uf_nome]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasNome</parent-property>
					</literal>
					<literal>
						<value><![CDATA[uf_regiao]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasRegiao</parent-property>
					</literal>
				</dynamic-literal>
			</dynamic-literals>
		</resource>
		
		<resource id="2" type="static"  separator-char="," with-header="true">
			<uri>http://greco.ppgi.ufrj.br/resource#Municipio</uri>
			<rdf-type>http://greco.ppgi.ufrj/ontology/concepts#Municipio</rdf-type>
			
			<headers>
				<header>
					<index>2</index>
					<name>municipio_uri</name>
				</header>
				<header>
					<index>5</index>
					<name>municipio_nome</name>
				</header>
				<header>
					<index>7</index>
					<name>uf_regiao</name>
				</header>
			</headers>
			
			<static-literals>
				<static-literal>
					<value>Divisão territorial dos Estados do Brasil</value>
					<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasDescricao</parent-property>
				</static-literal>
			</static-literals>

			<dynamic-literals>
				<dynamic-literal service-id="2">
					<group>municipio_nome,uf_regiao</group>

					<literal>
						<value><![CDATA[municipio_uri]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasURI</parent-property>
					</literal>
					<literal>
						<value><![CDATA[municipio_nome]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasNome</parent-property>
					</literal>
					<literal>
						<value><![CDATA[uf_regiao]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasRegiao</parent-property>
					</literal>
				</dynamic-literal>
			</dynamic-literals>
			
 			<object-properties>
				<object-property type="nondominant">
					<property>http://greco.ppgi.ufrj/ontology/properties#hasSubDivisao</property>
					<object-uri>http://greco.ppgi.ufrj.br/resource#Estado</object-uri>
				</object-property>
			</object-properties> 
		</resource>
		
		<resource id="3" type="static"  separator-char="," with-header="true">
			<uri>http://greco.ppgi.ufrj.br/resource#IBGE</uri>
			<rdf-type>http://greco.ppgi.ufrj.br/ontology/concepts#IBGE</rdf-type>
			
			<static-literals>
				<static-literal>
					<value>Instituto Brasileiro de Geografia e Estatística</value>
					<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasNome</parent-property>
				</static-literal>
			</static-literals>
			<headers>
			</headers>
			<dynamic-literals>
			</dynamic-literals>
			
			<object-properties>
				<object-property type="dominant">
					<property>http://greco.ppgi.ufrj/ontology/properties#hasDados</property>
					<object-uri>http://greco.ppgi.ufrj.br/resource#Municipio</object-uri>
				</object-property>
			</object-properties>
		</resource>
		
		<resource id="4" type="dynamic"  separator-char="," with-header="true">
			<uri>http://greco.ppgi.ufrj.br/resource/AcidenteDeTrabalho#</uri>
			<rdf-type>http://greco.ppgi.ufrj.br/ontology/concepts#AcidentesTrabalho</rdf-type>
			
			<static-literals>
			</static-literals>
			
			<headers>
				<header>
					<index>1</index>
					<name>ano</name>
				</header>
				<header>
					<index>3</index>
					<name>municipio_nome</name>
				</header>
				<header>
					<index>4</index>
					<name>uf</name>
				</header>
				<header>
					<index>5</index>
					<name>acidentes_com_cat_tipicos</name>
				</header>
				<header>
					<index>6</index>
					<name>acidentes_com_cat_trajeto</name>
				</header>
				<header>
					<index>7</index>
					<name>acidentes_com_cat_doenca_profissional</name>
				</header>
				<header>
					<index>8</index>
					<name>quantidade_obtidos</name>
				</header>
				<header>
					<index>9</index>
					<name>acidentes_sem_cat</name>
				</header>
			</headers>
			
			<dynamic-literals>
				<dynamic-literal service-id="1" >
					<group>municipio_nome</group>
					<literal>
						<value><![CDATA[ano]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasAno</parent-property>
					</literal>
					<literal>
						<value><![CDATA[municipio_nome]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasNomeMunicipio</parent-property>
					</literal>
					<literal>
						<value><![CDATA[uf]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasUF</parent-property>
					</literal>
					<literal>
						<value><![CDATA[acidentes_com_cat_tipicos]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasAcidentesComCATTipicos</parent-property>
					</literal>
					<literal>
						<value><![CDATA[acidentes_com_cat_trajeto]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasAcidentesComCATTrajeto</parent-property>
					</literal>
					<literal>
						<value><![CDATA[acidentes_com_cat_doenca_profissional]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasAcidentesComCATDoencaProfissional</parent-property>
					</literal>
					<literal>
						<value><![CDATA[quantidade_obtidos]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasQuantidadeObitos</parent-property>
					</literal>
					<literal>
						<value><![CDATA[acidentes_sem_cat]]></value>
						<parent-property>http://greco.ppgi.ufrj/ontology/properties#hasAcidentesSemCAT</parent-property>
					</literal>
				</dynamic-literal>
			</dynamic-literals>
			
			<object-properties>
				<object-property type="nondominant">
					<property>http://greco.ppgi.ufrj/ontology/properties#hasOcorrencia</property>
					<object-uri>http://greco.ppgi.ufrj.br/resource#Municipio</object-uri>
				</object-property>
			</object-properties> 
			
		</resource>
		
		
	</resources>
</config>